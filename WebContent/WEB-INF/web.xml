<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.jsp </welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>learn.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  

  <servlet>
    <servlet-name>2</servlet-name>
    <servlet-class>learn.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>2</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>3</servlet-name>
    <servlet-class>learn.Questions</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>3</servlet-name>
    <url-pattern>/question</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>4</servlet-name>
    <servlet-class>learn.Leaderboard</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>4</servlet-name>
    <url-pattern>/Leaderboard</url-pattern>
  </servlet-mapping>
  
  
</web-app>